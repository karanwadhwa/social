.messages
  each type in Object.keys(messages)
    each message in messages[type]
      .message
        div(class = 'alert alert-dismissible alert-'+type)
          button.close(type='button', data-dismiss='alert', aria-hidden='true') Ã—
          if type == 'error'
            h4
              i.icon.fa.fa-warning
              |Error!
          if type == 'success'
            h4
              i.icon.fa.fa-check
              |Success
          |  #{message}
